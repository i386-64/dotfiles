{
    "[csharp]": {
        "editor.acceptSuggestionOnCommitCharacter": true,
        "editor.acceptSuggestionOnEnter": "on",
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.parameterHints.enabled": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": false
        },
        "editor.quickSuggestionsDelay": 10,
        "editor.suggest.localityBonus": true,
        "editor.suggestOnTriggerCharacters": true,
        "editor.suggestSelection": "recentlyUsed",
        "editor.tabCompletion": "off",
        "editor.wordBasedSuggestions": true
    },
    "[csv]": {},
    "[elixir]": {
        "editor.defaultFormatter": "JakeBecker.elixir-ls",
        "editor.formatOnType": true,
        "editor.insertSpaces": true,
        "editor.suggestOnTriggerCharacters": true,
        "editor.tabSize": 2,
        "editor.trimAutoWhitespace": false,
        "editor.wordBasedSuggestions": false,
        "editor.wordWrapColumn": 120,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "files.trimTrailingWhitespace": true
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "modifications",
        "editor.formatOnType": true,
        "editor.useTabStops": true
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.formatOnType": false
    },
    "[javascriptreact]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.formatOnType": false
    },
    "[json]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.suggestOnTriggerCharacters": true,
        "editor.tabSize": 2
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[ruby]": {},
    "[typescript]": {},
    "[typescriptreact]": {},
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.comments.ignoreEmptyLines": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "line",
    "editor.folding": false,
    "editor.foldingHighlight": false,
    "editor.fontFamily": "Cascadia Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.glyphMargin": true,
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineHeight": 1.75,
    "editor.lineNumbers": "off",
    "editor.links": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "auto",
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "[Default High Contrast Light]": {
            "textMateRules": [
                {
                    "scope": "comment.documentation",
                    "settings": {
                        "foreground": "#008000"
                    }
                },
                {
                    "scope": "comment.documentation.attribute",
                    "settings": {
                        "foreground": "#282828"
                    }
                },
                {
                    "scope": "comment.documentation.cdata",
                    "settings": {
                        "foreground": "#808080"
                    }
                },
                {
                    "scope": "comment.documentation.delimiter",
                    "settings": {
                        "foreground": "#808080"
                    }
                },
                {
                    "scope": "comment.documentation.name",
                    "settings": {
                        "foreground": "#808080"
                    }
                }
            ]
        },
        "[Default High Contrast]": {
            "textMateRules": [
                {
                    "scope": "comment.documentation",
                    "settings": {
                        "foreground": "#608B4E"
                    }
                },
                {
                    "scope": "comment.documentation.attribute",
                    "settings": {
                        "foreground": "#C8C8C8"
                    }
                },
                {
                    "scope": "comment.documentation.cdata",
                    "settings": {
                        "foreground": "#E9D585"
                    }
                },
                {
                    "scope": "comment.documentation.delimiter",
                    "settings": {
                        "foreground": "#808080"
                    }
                },
                {
                    "scope": "comment.documentation.name",
                    "settings": {
                        "foreground": "#569CD6"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "comment.documentation",
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.useTabStops": false,
    "editor.wordWrap": "on",
    "elixirLS.dialyzerEnabled": false,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "explorer.autoReveal": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "never",
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "history": false,
            "remote": false
        },
        "editorGroup": false,
        "editorTab": false,
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scm": {
            "authors": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true
        },
        "scmGroupInline": {
            "stash": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false,
            "stash": true
        }
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.gpuAcceleration": "on",
    "window.enableMenuBarMnemonics": true,
    "window.titleBarStyle": "custom",
    "workbench.colorCustomizations": {
        // "editor.background": "#000",
        // "editorCursor.foreground": "#f0f",
        // "editorGutter.addedBackground": "#0000",
        // "editorGutter.deletedBackground": "#0000",
        // "editorGutter.modifiedBackground": "#0000",
        // "sideBar.background": "#000"
    },
    "workbench.colorTheme": "Red",
    "workbench.editor.showTabs": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    "debug.console.fontSize": 11,
    "window.zoomLevel": 1
}
